{
  "manifest_version": 3,
  "name": "nicodel",
  "version": "1.0.0",
  "description": "Hide specific contributors.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
	},

	"permissions": [
		"storage"
	],

	"content_scripts": [
		{
			"matches": ["https://www.nicovideo.jp/my/*"],
			"js": ["front/index.js"]
		}
	],

	"options_ui": {
  	"page": "option/option.html",
		"open_in_tab": true
	}
}